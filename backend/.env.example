# EDNA Environment Variables
# Copy this file to .env and fill in your actual credentials
# DO NOT commit .env to version control!

# Default credentials
EDNA_DEFAULT_USERNAME=admin
EDNA_DEFAULT_PASSWORD=your_default_password

# Cisco devices
CISCO_USERNAME=admin
CISCO_PASSWORD=your_cisco_password

# MikroTik devices
MIKROTIK_USERNAME=mkadministrator
MIKROTIK_PASSWORD=your_mikrotik_password

# Fortinet devices
FORTINET_USERNAME=admin
FORTINET_PASSWORD=your_fortinet_password
